<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbHomeRoot" maxHeight="720.0" maxWidth="1280.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jptv22shopfx.HomeController">
   <children>
      <MenuBar fx:id="mbEnter" prefHeight="0.0" prefWidth="1280.0">
        <menus>
          <Menu mnemonicParsing="false" text="Shop">
            <items>
              <MenuItem fx:id="mbProductsAddNewProduct" mnemonicParsing="false" onAction="#clickMenuAddNewProduct" text="Add new product" />
                  <MenuItem fx:id="mbProductsTableProducts" mnemonicParsing="false" onAction="#clickMenuTableProducts" text="Table products" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Users">
            <items>
              <MenuItem fx:id="mbAddNewUser" mnemonicParsing="false" onAction="#clickMenuAddNewUser" text="New User" />
                  <MenuItem fx:id="mbEditPrfile" mnemonicParsing="false" onAction="#clickMenuEditProfile" text="Profile" />
                  <MenuItem fx:id="mbLogin" mnemonicParsing="false" onAction="#clickMenuLogin" text="Log in" />
                  <MenuItem fx:id="mbLogout" mnemonicParsing="false" onAction="#clickMenuLogout" text="Log out" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Admin">
            <items>
              <MenuItem fx:id="mbAdminPane" mnemonicParsing="false" onAction="#clickMenuShowAdminpane" text="Admin's panel" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label fx:id="lbInfoUser" alignment="CENTER" prefHeight="45.0" prefWidth="1280.0">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label fx:id="lbInfoHome" alignment="CENTER" layoutX="10.0" layoutY="35.0" prefHeight="45.0" prefWidth="1280.0">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <VBox fx:id="vbHomeContent" prefHeight="612.0" prefWidth="1280.0">
         <children>
            <Label alignment="CENTER" prefHeight="178.0" prefWidth="1287.0" text="Welcome to the Shop JPTV22" textFill="#3d2723" wrapText="true">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</VBox>
